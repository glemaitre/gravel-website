  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "vite": "^5.0.0",
    "eslint": "^8.57.0",
    "@eslint/js": "^9.0.0",
    "eslint-plugin-vue": "^9.20.0",
    "prettier": "^3.2.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "typescript": "^5.6.3",
    "vue-tsc": "^2.1.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/parser": "^7.18.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "vitest": "^2.0.5",
    "@vitest/coverage-v8": "^2.0.5",
