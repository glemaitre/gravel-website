# Production Environment Configuration
# Copy this file to .env/production and update the values as needed

# Environment
ENVIRONMENT=production

# Storage Configuration
STORAGE_TYPE=s3
AWS_ACCESS_KEY_ID=your_production_access_key_here
AWS_SECRET_ACCESS_KEY=your_production_secret_key_here
AWS_S3_BUCKET=your_production_bucket_name
AWS_REGION=us-east-1

# Database Configuration
DB_HOST=prod-db-host
DB_PORT=5432
DB_NAME=cycling_production
DB_USER=prod_user
DB_PASSWORD=your_production_password_here

# Logging
LOG_LEVEL=WARNING
