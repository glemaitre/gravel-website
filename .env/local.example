# Local Development Environment Configuration
# Copy this file to .env/local and update the values as needed

# Environment
ENVIRONMENT=local

# Storage Configuration
STORAGE_TYPE=local
LOCAL_STORAGE_ROOT=./scratch/local_storage
LOCAL_STORAGE_BASE_URL=http://localhost:8000/storage

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=cycling
DB_USER=postgres
DB_PASSWORD=your_password_here

# AWS Configuration (not needed for local storage, but included for completeness)
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_S3_BUCKET=your_bucket_name
# AWS_REGION=us-east-1

# Logging
LOG_LEVEL=INFO
